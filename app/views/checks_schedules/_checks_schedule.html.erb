<div id="<%= dom_id checks_schedule %>">
  <p>
    <strong>Pet:</strong>
    <%= checks_schedule.pet.name %>
  </p>

  <p>
    <strong>Check:</strong>
    <%= checks_schedule.check.check_type %>
  </p>

  <p>
    <strong>Date created:</strong>
    <%= checks_schedule.date_created %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= checks_schedule.notes %>
  </p>

  <p>
    <strong>Performed:</strong>
    <%= checks_schedule.performed %>
  </p>

</div>
